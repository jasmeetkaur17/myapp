<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sample.notify" 
        version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Sample notifications</name>
    <description>Sample notifications app.</description>
    <author email="xxx@aaaa.com">XXXXXXXXXX</author>
    <content src="index.html" />
    <access origin="*" launch-external="yes"/>	
	<gap:plugin name="cordova-plugin-whitelist" spec="1.3.2" source="npm" />
    <plugin name="phonegap-plugin-push" spec="1.9.0" source="npm">
        <param name="SENDER_ID" value="713837838534" />
    </plugin>
    
    <allow-intent href="tel:*" />
	<allow-intent href="http:*" />
	<allow-intent href="https:*" />
    
	<preference name="stay-in-webview" value="false" />
	<preference name="orientation" value="portrait" />   
    <preference name="android-installLocation" value="auto" />
<icon src="icon/web_hi_res_512.png" />	
 	<icon src="icon/res/mipmap-mdpi/ic_launcher.png" gap:platform="android" gap:qualifier="mdpi" />	
 	<icon src="icon/res/mipmap-hdpi/ic_launcher.png" gap:platform="android" gap:qualifier="hdpi" />	
 	<icon src="icon/res/mipmap-xhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xhdpi" />	

</widget>
